inline emit {
    #18 deo
}

inline cr {
    #0a emit
}

inline halt {
    #010f deo
}

origin 0100
;main jmp2


word puts {
    &while {
        ldak emit
        inc2
        ldak ,&while jcn
    }
    cr
    pop2
}

@main {
    ;hello-world puts
    ;goodbye puts
    halt
}


@hello-world "Hello, world!"
@goodbye "Goodbye"

